    "version": "0.2.0",
    "configurations": [
        

        {
            "name": "Python : Fichier actuel",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python : Profile unit tests",
            "type": "python",
            "request": "launch",
            "module": "cProfile",
            "args": ["-o",  "marslander.cprof",  "runAllTests.py"]
        },
        {
            "name": "KCacheGrind : marslander.cprof",
            "type": "python",
            "request": "launch",
            "module": "pyprof2calltree",
            "args": ["-k",  "-i",  "marslander.cprof"]
        },
        {
            "name": "Python: Run unit tests",
            "type": "python",
            "request": "launch",
            "module": "tests"
        }
    ]
}
